{
  "name": "tidal-transit",
  "type": "module",
  "version": "0.0.1",
  "scripts": {
    "dev": "astro dev",
    "build": "astro build",
    "preview:astro": "astro preview",
    "preview:wrangler": "npm run build:dev && wrangler dev",
    "astro": "astro",
    "test": "vitest",
    "lint": "eslint . --ext .js,.ts,.astro",
    "lint:fix": "eslint . --ext .js,.ts,.astro --fix",
    "test:performance": "node ./scripts/lighthouse.mjs",
    "build:dev": "dotenv -e .env.development -- astro build",
    "build:prod": "dotenv -e .env.production -- astro build",
    "deploy:dev": "npm run build:dev && wrangler deploy --env development",
    "deploy:prod": "npm run build:prod && wrangler deploy --env production"
  },
  "dependencies": {
    "@astrojs/cloudflare": "^12.6.9",
    "@astrojs/mdx": "^4.3.5",
    "@astrojs/node": "^9.4.3",
    "@astrojs/rss": "^4.0.12",
    "@astrojs/sitemap": "^3.6.0",
    "astro": "^5.13.7",
    "sharp": "^0.34.3",
    "vite": "^7.1.6",
    "web-vitals": "^5.1.0"
  },
  "devDependencies": {
    "@eslint/js": "^9.35.0",
    "@typescript-eslint/eslint-plugin": "^8.44.0",
    "@typescript-eslint/parser": "^8.44.0",
    "chrome-launcher": "^1.2.0",
    "dotenv": "^17.2.2",
    "dotenv-cli": "^10.0.0",
    "eslint": "^9.35.0",
    "eslint-plugin-astro": "^1.3.1",
    "jsdom": "^27.0.0",
    "lighthouse": "^12.8.2",
    "typescript": "^5.9.2",
    "vitest": "^3.2.4",
    "wrangler": "^4.38.0",
    "xml2js": "^0.6.2"
  }
}
